{
	"/": {
		"controller": "Home",
		"method": "get_home"
	},
	"/register": {
		"controller": "User",
		"method": "register"
	},
	"/login": {
		"controller": "User",
		"method": "login"
	},
	"/logout": {
		"controller": "User",
		"method": "logout"
	},
	"/admin/dashboard": {
		"controller": "Admin",
		"method": "panel"
	},
	"/admin/settings": {
		"controller": "Admin",
		"method": "settings"
	},
	"/admin/settings/:opt": {
		"controller": "Admin",
		"method": "settings",
		"params": {
			"opt": "string"
		}
	},
	"/customer/dashboard": {
		"controller": "User",
		"method": "go_to_dashboard"
	},
	"/vehicle/:id": {
		"controller": "Vehicle",
		"method": "get_vehicle",
		"params": {
			"id": "number"
		}
	},
	"/dashboard/vehicle/add": {
		"controller": "Admin",
		"method": "add_vehicle",
		"access": "admin"
	}
}
