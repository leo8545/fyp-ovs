{
	"/": {
		"controller": "Home",
		"method": "get_home"
	},
	"/register": {
		"controller": "Customer",
		"method": "register"
	},
	"/login": {
		"controller": "User",
		"method": "login"
	},
	"/logout": {
		"controller": "User",
		"method": "logout"
	},
	"/admin": {
		"controller": "Admin",
		"method": "dashboard"
	},
	"/admin/users": {
		"controller": "UserManager",
		"method": "get_users",
		"access": "admin"
	},
	"/admin/users/all": {
		"controller": "UserManager",
		"method": "get_users",
		"access": "admin"
	},
	"/admin/users/add": {
		"controller": "UserManager",
		"method": "add_user",
		"access": "admin"
	},
	"/admin/users/edit/:id": {
		"controller": "UserManager",
		"method": "edit_user",
		"params": {
			"id": "number"
		},
		"access": "admin"
	},
	"/admin/users/delete/:id": {
		"controller": "UserManager",
		"method": "delete_user",
		"params": {
			"id": "number"
		},
		"access": "admin"
	},
	"/admin/vehicles": {
		"controller": "VehicleManager",
		"method": "get_vehicles",
		"access": "admin"
	},
	"/admin/vehicles/all": {
		"controller": "VehicleManager",
		"method": "get_vehicles",
		"access": "admin"
	},
	"/admin/vehicles/add": {
		"controller": "VehicleManager",
		"method": "add_vehicle",
		"access": "admin"
	},
	"/admin/vehicles/edit/:id": {
		"controller": "VehicleManager",
		"method": "edit_vehicle",
		"params": {
			"id": "number"
		},
		"access": "admin"
	},
	"/admin/vehicles/delete/:id": {
		"controller": "VehicleManager",
		"method": "delete_vehicle",
		"params": {
			"id": "number"
		},
		"access": "admin"
	},
	"/admin/vehicles/models": {
		"controller": "VehicleManager",
		"method": "get_vehicle_models",
		"access": "admin"
	},
	"/admin/dashboard": {
		"controller": "Admin",
		"method": "dashboard"
	},
	"/admin/appearance": {
		"controller": "Admin",
		"method": "appearance"
	},
	"/admin/settings": {
		"controller": "Admin",
		"method": "settings"
	},
	"/admin/settings/:opt": {
		"controller": "Admin",
		"method": "settings",
		"params": {
			"opt": "string"
		}
	},
	"/customer/dashboard": {
		"controller": "User",
		"method": "go_to_dashboard"
	},
	"/vehicle/:id": {
		"controller": "Vehicle",
		"method": "get_vehicle",
		"params": {
			"id": "number"
		}
	}
}
